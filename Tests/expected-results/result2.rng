        présentation utilisés pour cet élément dans le texte source</a:documentation>
            <list>
               <data type="anyURI"/>
               <zeroOrMore>
                  <data type="anyURI"/>
               </zeroOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <define name="att.global.attribute.xmlbase">
      <optional>
         <attribute name="xml:base">
